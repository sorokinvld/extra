{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "api/main.py" }],
  "env": {
    "FLASK_APP": "api/main.py",
    "FLASK_ENV": "production"
  },
  "install": {
    "ignoreScripts": true
  },
  "headers": [
    {
      "source": "../public/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
